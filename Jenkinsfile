pipeline {

  agent any

  stages {

    stage("Build Docker Image") {
      steps {
        echo "Building Docker Image..."
      }
    }

    stage("Tag Docker Image") {
      steps {
        echo "Tagging Docker Image..."
      }
    }

    stage("Push Image to Repository") {
      steps {
        echo "Pushing Image to Repository..."
      }
    }

    stage("Deploy") {
      steps {
        echo "Deploying..."
      }
    }

  }

}